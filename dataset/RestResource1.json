{
	"name": "RestResource1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "OpenWeatherREST",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"lat": {
				"type": "float",
				"defaultValue": 42.3601
			},
			"lon": {
				"type": "float",
				"defaultValue": -71.0539
			},
			"start": {
				"type": "string",
				"defaultValue": "@div(sub(ticks(utcNow()), ticks('1970-01-01T00:00:00Z')), 10000000)"
			},
			"end": {
				"type": "string",
				"defaultValue": "@div(sub(ticks(addDays(utcNow(), -7)), ticks('1970-01-01T00:00:00Z')), 10000000)"
			},
			"appid": {
				"type": "string",
				"defaultValue": "@activity(\"Web1\").output.value"
			}
		},
		"annotations": [],
		"type": "RestResource",
		"typeProperties": {
			"relativeUrl": {
				"value": "data/3.0/history/city?lat=@{dataset().lat}&lon=@{dataset().lon}&start=@{dataset().start}&end=@{dataset().end}&appid=@{dataset().appid}",
				"type": "Expression"
			}
		},
		"schema": []
	}
}